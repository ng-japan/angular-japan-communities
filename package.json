{
  "name": "community-angular-jp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng run app:build-static",
    "bundlesize": "bundlesize",
    "format": "prettier --write 'src/**/*.{ts,scss,html}'",
    "format:check": "prettier --check 'src/**/*.{ts,scss,html}'",
    "lint": "ng lint && yarn format:check",
    "start": "ng serve app",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "tools:report-bundle-size": "./tools/report-bundle-size.mjs",
    "postinstall": "ngcc"
  },
  "dependencies": {
    "@angular/animations": "12.0.4",
    "@angular/cdk": "12.0.4",
    "@angular/common": "12.0.4",
    "@angular/compiler": "12.0.4",
    "@angular/core": "12.0.4",
    "@angular/forms": "12.0.4",
    "@angular/platform-browser": "12.0.4",
    "@angular/platform-browser-dynamic": "12.0.4",
    "@angular/router": "12.0.4",
    "@ngneat/tailwind": "7.0.3",
    "@nguniversal/common": "12.1.0-next.0",
    "comlink": "4.3.1",
    "core-js": "3.14.0",
    "remark": "13.0.0",
    "remark-html": "13.0.1",
    "rxjs": "6.6.7",
    "tslib": "2.3.0",
    "web-animations-js": "2.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.4",
    "@angular-eslint/builder": "12.1.0",
    "@angular-eslint/eslint-plugin": "12.1.0",
    "@angular-eslint/eslint-plugin-template": "12.1.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "12.1.0",
    "@angular/cli": "12.0.4",
    "@angular/compiler-cli": "12.0.4",
    "@angular/language-service": "12.0.4",
    "@nguniversal/builders": "12.1.0-next.0",
    "@testing-library/angular": "10.8.2",
    "@testing-library/jest-dom": "5.14.1",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.3",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "autoprefixer": "10.2.6",
    "eslint": "7.28.0",
    "filesize": "6.3.0",
    "globby": "11.0.3",
    "jest": "27.0.4",
    "jest-preset-angular": "9.0.4",
    "postcss": "8.3.4",
    "prettier": "2.3.1",
    "tailwindcss": "2.1.4",
    "ts-jest": "27.0.3",
    "typescript": "4.2.4",
    "zx": "1.14.1"
  },
  "bundlesize": [
    {
      "path": "./dist/app/*.{js,css}",
      "compression": "none"
    }
  ]
}
